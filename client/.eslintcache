/ Note that as the email and password are captured, the JWT package will add token to it\r\n        const { data } = await axios.post(\"/auth/login\", creds);\r\n        // Put the e-mail and token in a state\r\n        // This is done by first destructuring email and token from data\r\n        const { email, token }= data;\r\n        const apiToken = token;\r\n        console.log(apiToken)\r\n        dispatch ({\r\n            type:\"LOGIN\", \r\n            email, \r\n            apiToken: data.token });\r\n\r\n        }\r\n// This portion of the loginForm function is written first\r\n    \r\n        return (\r\n\r\n                <Form onSubmit={handleSubmit}>\r\n                <Form.Group controlId=\"formBasicEmail\">\r\n                    <Form.Label>Email address</Form.Label>\r\n                    <Form.Control type=\"email\" placeholder=\"Enter email\" ref={emailRef} />\r\n                    <Form.Text className=\"text-muted\">\r\n                    We'll never share your email with anyone else.\r\n                    </Form.Text>\r\n                </Form.Group>\r\n\r\n                <Form.Group controlId=\"formBasicPassword\">\r\n                    <Form.Label>Password</Form.Label>\r\n                    <Form.Control type=\"password\" placeholder=\"Password\"  ref={passwordRef} />\r\n                </Form.Group>\r\n                <Form.Group controlId=\"formBasicCheckbox\">\r\n                    <Form.Check type=\"checkbox\" label=\"Check me out\" />\r\n                </Form.Group>\r\n                <Button variant=\"primary\" type=\"submit\">\r\n                    Submit\r\n                </Button>\r\n                </Form>\r\n\r\n\r\n\r\n\r\n\r\n\r\n            // <form onSubmit={handleSubmit}>\r\n            //   <input type=\"text\" placeholder=\"email\" ref={emailRef} />\r\n            //   <input type=\"password\" placeholder=\"password\"  ref={passwordRef} />\r\n            //   <button type=\"submit\">Login</button>\r\n            // </form>\r\n          ) \r\n    \r\n}\r\n\r\nexport default LoginForm;\r\n\r\n\r\n","C:\\Users\\jidre\\Desktop\\usra.tech\\client\\src\\components\\AuthenticatedApp.js",[],"C:\\Users\\jidre\\Desktop\\usra.tech\\client\\src\\components\\UnauthenticatedApp.js",["61"],"import React from \"react\";\r\nimport LoginForm from './LoginForm'\r\nimport Headbar from \"./Headbar/Headbar\";\r\nimport Jumbo from \"./Jumbo/Jumbo\";\r\nimport '../App.css';\r\n\r\nfunction UnauthenticatedApp() {\r\n    return (\r\n      <div >\r\n       \r\n    <Headbar />\r\n    <Jumbo />\r\n\r\n       {/* <LoginForm /> */}\r\n\r\n      </div>\r\n    );\r\n  }\r\n  \r\n  export default UnauthenticatedApp;\r\n  ","C:\\Users\\jidre\\Desktop\\usra.tech\\client\\src\\components\\Headbar\\Headbar.js",["62"],"import React from \"react\";\r\nimport Navbar from 'react-bootstrap/Navbar'\r\nimport Nav from 'react-bootstrap/Nav'\r\nimport NavDropdown from 'react-bootstrap/NavDropdown'\r\n\r\nfunction Headbar() {\r\n  return (\r\n \r\n    <Navbar collapseOnSelect expand=\"lg\" bg=\"primary\" variant=\"dark\">\r\n    <Navbar.Brand href=\"#home\">USRA.Tech, Discover the Truth Beyond Numbers</Navbar.Brand>\r\n    <Navbar.Toggle aria-controls=\"responsive-navbar-nav\" />\r\n    <Navbar.Collapse id=\"responsive-navbar-nav\">\r\n      <Nav className=\"mr-auto\">\r\n        \r\n      </Nav>\r\n\r\n      \r\n      <Nav>\r\n        <Nav.Link href=\"#deets\">Services</Nav.Link>\r\n        <Nav.Link href=\"#deets\">Contact</Nav.Link>\r\n        <Nav.Link href=\"#deets\">About</Nav.Link>\r\n       \r\n      </Nav>\r\n    </Navbar.Collapse>\r\n  </Navbar>\r\n\r\n  );\r\n}\r\n\r\nexport default Headbar","C:\\Users\\jidre\\Desktop\\usra.tech\\client\\src\\components\\Jumbo\\Jumbo.js",["63"],"import React from \"react\";\r\nimport Jumbotron from 'react-bootstrap/Jumbotron'\r\nimport {Button } from 'react-bootstrap'\r\nimport FormModal from '../Modal/FormModal'\r\nimport Image from 'react-bootstrap/Image'\r\nimport \"../../Ap