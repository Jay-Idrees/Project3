 LoginForm;\r\n\r\n// \r\n//","C:\\Users\\jidre\\Desktop\\usra.tech\\client\\src\\components\\AuthenticatedApp.js",[],"C:\\Users\\jidre\\Desktop\\usra.tech\\client\\src\\components\\UnauthenticatedApp.js",[],"C:\\Users\\jidre\\Desktop\\usra.tech\\client\\src\\components\\Nav.js",["44","45","46","47","48","49","50","51"],{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","severity":1,"message":"57","line":4,"column":27,"nodeType":"58","messageId":"59","endLine":4,"endColumn":37},{"ruleId":"56","severity":1,"message":"60","line":5,"column":8,"nodeType":"58","messageId":"59","endLine":5,"endColumn":12},{"ruleId":"56","severity":1,"message":"61","line":9,"column":8,"nodeType":"58","messageId":"59","endLine":9,"endColumn":17},{"ruleId":"62","severity":1,"message":"63","line":23,"column":4,"nodeType":"64","endLine":23,"endColumn":20,"suggestions":"65"},{"ruleId":"56","severity":1,"message":"66","line":17,"column":16,"nodeType":"58","messageId":"59","endLine":17,"endColumn":17},{"ruleId":"67","severity":1,"message":"68","line":6,"column":7,"nodeType":"69","endLine":6,"endColumn":44},{"ruleId":"67","severity":1,"message":"68","line":23,"column":13,"nodeType":"69","endLine":23,"endColumn":46},{"ruleId":"67","severity":1,"message":"68","line":28,"column":13,"nodeType":"69","endLine":28,"endColumn":46},{"ruleId":"67","severity":1,"message":"68","line":33,"column":13,"nodeType":"69","endLine":41,"endColumn":14},{"ruleId":"67","severity":1,"message":"68","line":45,"column":15,"nodeType":"69","endLine":45,"endColumn":53},{"ruleId":"67","severity":1,"message":"68","line":48,"column":15,"nodeType":"69","endLine":48,"endColumn":53},{"ruleId":"67","severity":1,"message":"68","line":52,"column":15,"nodeType":"69","endLine":52,"endColumn":53},{"ruleId":"67","severity":1,"message":"68","line":58,"column":13,"nodeType":"69","endLine":58,"endColumn":55},"no-native-reassign",["70"],"no-negated-in-lhs",["71"],"no-unused-vars","'useReducer' is defined but neve